            <Shape><Polygon POINTS="304 552 304 495 1836 500 1836 557 1834 572 302 567"/></Shape>
	    <String CONTENT="وسببه توسط الأرض بينه وبين الشمس، فإذا كان القمر في إحدى نقطتي الرأس"
                    HPOS="302"
                    VPOS="495"
                    WIDTH="1534"
                    HEIGHT="77"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_16c700e5"
                    
                    BASELINE="309 640 1954 640" 
                    HPOS="309"
                    VPOS="586"
                    WIDTH="1645"
                    HEIGHT="69">
            <Shape><Polygon POINTS="309 640 309 586 1954 586 1954 640 1954 655 309 655"/></Shape>
	    <String CONTENT="والذنب أو قريباً منه عند الاستقبال تتوسط الأرض بينه وبين الشمس، فيقع في ظل"
                    HPOS="309"
                    VPOS="586"
                    WIDTH="1645"
                    HEIGHT="69"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_b0a12f78"
                    
                    BASELINE="314 719 886 716 1699 734 1954 724" 
                    HPOS="311"
                    VPOS="657"
                    WIDTH="1643"
                    HEIGHT="94">
            <Shape><Polygon POINTS="314 719 311 660 884 657 886 657 1117 662 1696 675 1952 665 1954 724 1954 741 1699 751 1696 751 1114 738 884 734 314 736"/></Shape>
	    <String CONTENT="الأرض، ويبقى على سواده الأصلى&quot; فيرى منخسفاً، والشمس أعظم من الأرض،"
                    HPOS="311"
                    VPOS="657"
                    WIDTH="1643"
                    HEIGHT="94"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_b60357c3"
                    
                    BASELINE="314 802 550 795 1954 810" 
                    HPOS="311"
                    VPOS="738"
                    WIDTH="1643"
                    HEIGHT="94">
            <Shape><Polygon POINTS="314 802 311 746 547 738 550 738 1954 753 1954 810 1952 832 550 817 314 824"/></Shape>
	    <String CONTENT="فيكون ظل الشمس مخروطاً قاعدته دائرة صفحة الأرض&quot; لان الخطوط الشعاعية التى"
                    HPOS="311"
                    VPOS="738"
                    WIDTH="1643"
                    HEIGHT="94"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_9fa428ec"
                    
                    BASELINE="309 896 515 886 1362 900 1959 891" 
                    HPOS="304"
                    VPOS="824"
                    WIDTH="1655"
                    HEIGHT="96">
            <Shape><Polygon POINTS="309 896 304 834 510 824 515 824 970 832 1362 839 1957 829 1959 891 1959 910 1362 920 1360 920 965 915 515 905 309 915"/></Shape>
	    <String CONTENT="تخرج من الشمس إلى جرم الأرض لا تكون متوازية، فإذا اقصلت بمحيط الأرض"
                    HPOS="304"
                    VPOS="824"
                    WIDTH="1655"
                    HEIGHT="96"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_096ddb86"
                    
                    BASELINE="311 969 1959 982" 
                    HPOS="309"
                    VPOS="915"
                    WIDTH="1650"
                    HEIGHT="84">
            <Shape><Polygon POINTS="311 969 311 915 1959 927 1959 982 1957 999 309 986"/></Shape>
	    <String CONTENT="ونقذت في الجهة الأخرى تلاقياً عند نقطة فيعحصل ظل الأرض على شكل المخروط،"
                    HPOS="309"
                    VPOS="915"
                    WIDTH="1650"
                    HEIGHT="84"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_3222d9b5"
                    
                    BASELINE="309 1070 574 1060 1952 1065" 
                    HPOS="306"
                    VPOS="1006"
                    WIDTH="1646"
                    HEIGHT="84">
            <Shape><Polygon POINTS="309 1070 306 1016 572 1006 574 1006 1952 1011 1952 1065 1949 1085 574 1080 309 1090"/></Shape>
	    <String CONTENT="فإذا لم يكن للقمر عرض عن فلك البروج عند الاستقبال، وقع كله في جرم"
                    HPOS="306"
                    VPOS="1006"
                    WIDTH="1646"
                    HEIGHT="84"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_5831aa39"
                    
                    BASELINE="306 1151 1959 1144" 
                    HPOS="304"
                    VPOS="1097"
                    WIDTH="1655"
                    HEIGHT="69">
            <Shape><Polygon POINTS="306 1151 304 1104 1957 1097 1959 1144 1959 1158 306 1166"/></Shape>
	    <String CONTENT="المخروط، فيخسف كله حينئد، وان كان له عرض پخسف يعضه، وربسا يماس جرم"
                    HPOS="304"
                    VPOS="1097"
                    WIDTH="1655"
                    HEIGHT="69"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_01f2550c"
                    
                    BASELINE="306 1227 1962 1227" 
                    HPOS="306"
                    VPOS="1183"
                    WIDTH="1656"
                    HEIGHT="69">
            <Shape><Polygon POINTS="306 1227 306 1183 1962 1183 1962 1227 1962 1252 306 1252"/></Shape>
	    <String CONTENT="القمر مخروط الظل، ولا يقع فيه شيء، وذلك إذا كان عرض القمر مساوياً لنصف"
                    HPOS="306"
                    VPOS="1183"
                    WIDTH="1656"
                    HEIGHT="69"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_479baea2"
                    
                    BASELINE="309 1323 1964 1313" 
                    HPOS="306"
                    VPOS="1259"
                    WIDTH="1658"
                    HEIGHT="83">
