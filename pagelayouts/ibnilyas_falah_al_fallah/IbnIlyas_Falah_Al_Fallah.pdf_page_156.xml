            <Shape><Polygon POINTS="286 514 283 456 2207 450 2210 508 2210 534 286 540"/></Shape>
	    <String CONTENT="مثاقیل بزربنخ وأربعة مثاقیل لبن عشار وصببت علیه من الماء ما یعلوه باصعین وترکت"
                    HPOS="283"
                    VPOS="450"
                    WIDTH="1927"
                    HEIGHT="90"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_3856f339"
                    
                    BASELINE="280 593 2204 602" 
                    HPOS="277"
                    VPOS="540"
                    WIDTH="1927"
                    HEIGHT="88">
            <Shape><Polygon POINTS="280 593 280 540 2204 549 2204 602 2201 628 277 619"/></Shape>
	    <String CONTENT="في الشمس يومين وليلتين ولا تزال تسوطه بعود حتى يخرج طعمه، ثم اطرح فيه حب الحنطة"
                    HPOS="277"
                    VPOS="540"
                    WIDTH="1927"
                    HEIGHT="88"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_8768e75a"
                    
                    BASELINE="286 695 2207 686" 
                    HPOS="283"
                    VPOS="634"
                    WIDTH="1924"
                    HEIGHT="88">
            <Shape><Polygon POINTS="286 695 283 643 2204 634 2207 686 2207 713 286 722"/></Shape>
	    <String CONTENT="ودعه فيه يوما وليلة ثم أخرجه وانثر منه عند مجتمع الطير من الزرع فانه اذا اكل من صرع"
                    HPOS="283"
                    VPOS="634"
                    WIDTH="1924"
                    HEIGHT="88"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_3b3a8dfe"
                    
                    BASELINE="283 786 2210 780" 
                    HPOS="280"
                    VPOS="727"
                    WIDTH="1930"
                    HEIGHT="82">
            <Shape><Polygon POINTS="283 786 280 733 2207 727 2210 780 2210 803 283 809"/></Shape>
	    <String CONTENT="وان لم تدركه بیشیء من الزيت تسقيه والا مات، ورأیت في موضع آخر ما هو آقرب منه وه"
                    HPOS="280"
                    VPOS="727"
                    WIDTH="1930"
                    HEIGHT="82"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_41981f10"
                    
                    BASELINE="283 874 2210 865" 
                    HPOS="280"
                    VPOS="815"
                    WIDTH="1930"
                    HEIGHT="88">
            <Shape><Polygon POINTS="283 874 280 824 2207 815 2210 865 2210 894 283 903"/></Shape>
	    <String CONTENT="آن ینقع قمح في ماء زرنیخ وکبریت یومین ولیلتین ولاتزال تسوطه بعود، وحتی یخرچ حعمه"
                    HPOS="280"
                    VPOS="815"
                    WIDTH="1930"
                    HEIGHT="88"></String>
          </TextLine>

          
          
          <TextLine ID="eSc_line_678d6159"
                    
                    BASELINE="280 961 1683 970 2215 955" 
                    HPOS="277"
                    VPOS="894"
                    WIDTH="1938"
                    HEIGHT="108">
